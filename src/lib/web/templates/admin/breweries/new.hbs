  <button>Back</button>
</a>
<form action="/admin/breweries" method="post">
  <button type="submit" class="">Add Brewery</button>
  </br>
  <label for="name">Name</label>
  <input type="text" id="name" name="name" placeholder="i. e. Ol' Watering Hole">
  </br>
  <label for="city">City</label>
  <input type="text" id="city" name="city" placeholder="i. e. East Nowherelington">
  </br>
  <label for="latitude">Latitude</label>
  <input type="text" id="latitude" name="latitude" placeholder="i. e. 0"></input>
  </br>
  <label for="longitude">Longitude</label>
  <input type="text" id="longitude" name="longitude" placeholder="i. e. 180"></input>
</form>
